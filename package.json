{
  "name": "energee-api",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.7.0",
    "npm": "6.10.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "mocha --require @babel/register './src/test/**/*.js' --recursive --timeout 30000",
    "start": "npm run prod",
    "heroku-postbuild": "npm install --production",
    "dev": "cross-env NODE_ENV=development DEBUG=energee-api:server npm run watch ./src/bin/www",
    "prod": "npm run build && NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai-arrays": "^2.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.12",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.0.1",
    "nodemon": "^1.19.4"
  }
}
